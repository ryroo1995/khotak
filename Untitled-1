        SELECT  universty.name , universty.contry , universty.address , universty.type , subject.name  FROM universty,subject_universty,subject WHERE universty.univID=subject_universty.univID AND subject.subjectID=subject_universty.subjectID AND universty.name = '' AND universty.contry ='' and universty.type ='' 
        
         SELECT  universty.name , universty.contry , universty.address , universty.type , subject.name  FROM universty,subject_universty,subject WHERE universty.univID=subject_universty.univID AND subject.subjectID=subject_universty.subjectID AND subject.name = ''